pipeline {
    agent any
    tools { 
        maven 'maven3.6' 
        jdk 'jdk8' 
    }
    stages {
        stage('Unit Tests') {
            steps{
                sh 'mvn -f maths/pom.xml clean test'
            }
        }

        stage ('Build') {
            steps {
                sh 'mvn -f maths/pom.xml package'
            }
        }
    }
}
